<h1 class="text-black">ログインページ</h1>
<%= form_with url: login_path do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.email_field :email, class: "form-control" %>
  </div>

  <div>
  <%= f.label :password %>
  <%= f.password_field :password, class: "form-control" %>
  </div>

  <%= link_to 'Login', root_path %>
<% end %>
